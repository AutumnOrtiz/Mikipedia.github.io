<body>
  <nav>
    <div class="nav-container">
      <a href="/show">List</a>
      <a href="/">Create</a>
      <a href="/edit">Edit</a>
      <div id="indicator"></div>
    </div>
       
    <form class="form-inline" method ="POST" action ="/search" method="PUT">
     
      <input class="form-control mr-sm-2 search" type="search" placeholder="Search" aria-label="Search" type="text" id="search" name="search"><br><br>
      <button class="btn  submitbtn" type="submit">Search</button>
  </form>

  </nav>




  <div id="page" class="page">
    <div id="image"></div>
    <h1>Upload Character</h1>
    <form class="form" id="form" action="/" method="POST">
      <div class=" first">
        <label class="label" for="fname">First name:</label><br>
        <input type="text" id="fname" name="fname">
      </div><br>

      <div class="last">
        <label class="label" for="lname">Last name:</label> <br>
        <input type="text" id="lname" name="lname">
      </div><br>

      <div class="age">
        <label class="label" for="age">Age:</label> <br>
        <input type="text" id="age" name="age">
      </div><br>

      <div class="height">
        <label class="label" for="height">Height:</label><br>
        <input type="text" id="height" name="height">
      </div><br>

      <div class="occupation">
        <label class="label" for="occ">Occupation:</label><br>
        <input type="text" id="occ" name="occ">
      </div><br>

      <div class="status">
        <label class="label" for="status">Status:</label><br>
        <input type="text" id="status" name="status">
      </div><br>

      <div class="img">
        <label class="label" for="img">Image:</label> <br>
        <input type="file" id="img" name="img">
      </div><br>

      <div class="desc">
        <label class="label" for="desc">Description:</label> <br>
        <input type="text" id="desc" name="desc">

        <input type="submit" value="submit" class="submit">
      </div>
    </form>
  </div>
  <div id="dots1"></div>
  <div id="dots2"></div>
</body>

